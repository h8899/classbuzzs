{
  "extends": "airbnb",
  parser: "babel-eslint",
  "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/forbid-prop-types": 0,
      "react/jsx-indent": 0,
      "react/jsx-indent-props": 0,
      "react/jsx-no-bind": 0,
      "indent": 0,
      "object-curly-newline": 0,
      "linebreak-style": 0,
      "no-underscore-dangle": ["error", { "allow": ["__navigatingToLink"] }],
      "jsx-a11y/anchor-is-valid": 0,
  },
  "settings": {
    "import/parser": "babel-eslint",
  },
  "env": {
    "browser": true,
  }
}
